	spec = null;
	testSubject.startClient(size, userMsg, sound, showOpeningVideo, savedGame, spec);
	
	// test 2
	testSubject=createTestSubject();spec = null;
	savedGame = null;
	testSubject.startClient(size, userMsg, sound, showOpeningVideo, savedGame, spec);
	
	// test 3
	testSubject=createTestSubject();savedGame = null;
	testSubject.startClient(size, userMsg, sound, showOpeningVideo, savedGame, spec);
	
	// test 4
	testSubject=createTestSubject();spec = null;
	testSubject.startClient(size, userMsg, sound, showOpeningVideo, savedGame, spec);
	
	Assert.fail();
	}

